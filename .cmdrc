#!/bin/bash

# todo --name ${scene}_${material}

q_all_objects () {
    for material in smooth shiny metal mirror; do 
        for scene in bunny sphere cube; do 
            q a40 -- bash run_split.sh datasets/renders/${scene}_${material}
        done
    done
}

q_all_objects_baseline () {
    for material in smooth shiny metal mirror; do 
        for scene in bunny sphere cube; do 
            q a40 -- LABEL=_baseline bash run.sh datasets/renders/${scene}_${material}
        done
    done
}

q_all_objects_baseline_no_sh () {
    for material in smooth shiny metal mirror; do 
        for scene in bunny sphere cube; do 
            q a40 -- LABEL=_baseline_no_sh bash run.sh datasets/renders/${scene}_${material} --diffuse_only
        done
    done
}
